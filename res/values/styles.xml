<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <declare-styleable name="FlipView">
        <attr name="orientation" format="enum">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
        </attr>
        <attr name="overFlipMode" format="enum">
            <enum name="glow" value="0" />
            <enum name="rubber_band" value="1" />
        </attr>
    </declare-styleable>

    <style name="menu">
        <item name="android:background">@drawable/checked_menu</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">3.0dip</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="NavPage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <!-- <item name="android:background">@drawable/nav_page</item> -->
        <item name="android:background">@color/A</item>
    </style>

    <style name="menu_tab">
        <item name="android:background">@drawable/bg_tabhei</item>
    </style>

    <style name="default_linerlayout">
        <item name="android:background">#00000000</item>
        <item name="android:padding">3.0dip</item>
        <item name="android:gravity">top|right</item>
    </style>

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

